using System;
using System.Linq;

class Solution
{
    static int lonelyinteger(int[] a)
    {
        int result = 0;
        foreach (int num in a)
            result ^= num;
        return result;
    }

    static void Main()
    {
        int n = int.Parse(Console.ReadLine());
        int[] arr = Array.ConvertAll(Console.ReadLine().Split(), int.Parse);
        Console.WriteLine(lonelyinteger(arr));
    }
}
