using System;

class Solution
{
    static void decentNumber(int n)
    {
        int fives = n;

        // reduce fives until divisible by 3
        while (fives % 3 != 0)
        {
            fives -= 5;
        }

        if (fives < 0)
        {
            Console.WriteLine("-1");
            return;
        }

        int threes = n - fives;

        // print the number: all 5s followed by all 3s
        Console.WriteLine(new string('5', fives) + new string('3', threes));
    }

    static void Main()
    {
        int t = int.Parse(Console.ReadLine());
        for (int i = 0; i < t; i++)
        {
            int n = int.Parse(Console.ReadLine());
            decentNumber(n);
        }
    }
}
