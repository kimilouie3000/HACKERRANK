using System;
using System.Linq;

class Solution
{
    static void CountApplesAndOranges(int s, int t, int a, int b, int[] apples, int[] oranges)
    {
        int appleCount = apples.Count(dist => a + dist >= s && a + dist <= t);
        int orangeCount = oranges.Count(dist => b + dist >= s && b + dist <= t);

        Console.WriteLine(appleCount);
        Console.WriteLine(orangeCount);
    }

    static void Main(string[] args)
    {
        // Read house start and end
        string[] st = Console.ReadLine().Split(' ');
        int s = int.Parse(st[0]);
        int t = int.Parse(st[1]);

        // Read apple tree and orange tree positions
        string[] ab = Console.ReadLine().Split(' ');
        int a = int.Parse(ab[0]);
        int b = int.Parse(ab[1]);

        // Read counts of apples and oranges
        string[] mn = Console.ReadLine().Split(' ');
        int m = int.Parse(mn[0]);
        int n = int.Parse(mn[1]);

        // Read apple distances
        int[] apples = Array.ConvertAll(Console.ReadLine().Split(' '), int.Parse);

        // Read orange distances
        int[] oranges = Array.ConvertAll(Console.ReadLine().Split(' '), int.Parse);

        // Call function
        CountApplesAndOranges(s, t, a, b, apples, oranges);
    }
}
