using System;
using System.Linq;

class Solution
{
    static string misereNim(int[] s)
    {
        // Check if all piles contain 1 stone
        bool allOnes = s.All(x => x == 1);

        if (allOnes)
        {
            // If all piles are size 1, winner depends on parity
            return (s.Length % 2 == 0) ? "First" : "Second";
        }
        else
        {
            // Otherwise use normal Nim rule
            int nimSum = 0;
            foreach (int stones in s)
                nimSum ^= stones;

            return (nimSum == 0) ? "Second" : "First";
        }
    }

    static void Main(string[] args)
    {
        int t = int.Parse(Console.ReadLine()); // number of test cases
        for (int i = 0; i < t; i++)
        {
            int n = int.Parse(Console.ReadLine()); // number of piles
            int[] s = Array.ConvertAll(Console.ReadLine().Split(' '), int.Parse);

            Console.WriteLine(misereNim(s));
        }
    }
}
