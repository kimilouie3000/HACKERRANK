using System;
using System.Linq;

class Solution
{
    static int runningTime(int[] arr)
    {
        int n = arr.Length;
        int shifts = 0;

        for (int i = 1; i < n; i++)
        {
            int value = arr[i];
            int j = i - 1;

            // Shift elements greater than value to the right
            while (j >= 0 && arr[j] > value)
            {
                arr[j + 1] = arr[j];
                j--;
                shifts++; // count each shift
            }

            // Insert value into the right position
            arr[j + 1] = value;
        }

        return shifts;
    }

    static void Main()
    {
        int n = int.Parse(Console.ReadLine());
        int[] arr = Console.ReadLine().Split().Select(int.Parse).ToArray();

        Console.WriteLine(runningTime(arr));
    }
}
