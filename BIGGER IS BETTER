using System;

class Solution
{
    static string biggerIsGreater(string w)
    {
        char[] arr = w.ToCharArray();
        int i = arr.Length - 2;

        // Step 1: Find the pivot (first character from right that is smaller than next)
        while (i >= 0 && arr[i] >= arr[i + 1])
            i--;

        if (i == -1)
            return "no answer";

        // Step 2: Find rightmost character greater than pivot
        int j = arr.Length - 1;
        while (arr[j] <= arr[i])
            j--;

        // Step 3: Swap pivot and successor
        char temp = arr[i];
        arr[i] = arr[j];
        arr[j] = temp;

        // Step 4: Reverse suffix
        Array.Reverse(arr, i + 1, arr.Length - (i + 1));

        return new string(arr);
    }

    static void Main(string[] args)
    {
        int T = int.Parse(Console.ReadLine());
        for (int t = 0; t < T; t++)
        {
            string w = Console.ReadLine();
            Console.WriteLine(biggerIsGreater(w));
        }
    }
}
